  "type": "library",
  "minimum-stability": "stable",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Marcus Bointon",
      "email": "phpmailer@synchromedia.co.uk"
    },
    {
      "name": "Franck Cassedanne",
      "email": "franck@ouarz.net"
    }
